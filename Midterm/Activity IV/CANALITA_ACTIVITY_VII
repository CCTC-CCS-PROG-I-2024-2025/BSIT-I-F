import java.util.Scanner;

public class CANALITA_ACTIVITY_VII {


  public static void main(String[] args) {
    Scanner SC = new Scanner(System.in);
    System.out.println("Enter a non-neagtive integer.");

    int num = sc.nextInt();


    int factorial = 1;


    for (int i = 1; i <= num; i++) {
        factorial *= i;
        
        System,out.println("The factorial of" + num + "is" + factorial);
    }
  }

}